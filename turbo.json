{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false
    },
    "@repo/db#dev": {
      "outputs": ["dist/**"]
    },
    "api#dev": {
      "dependsOn": ["^dev", "@repo/db#dev"]
    },
    "web#dev": {
      "dependsOn": ["^dev", "api#dev"]
    },
    "native#dev": {
      "dependsOn": ["^dev", "api#dev"]
    }
  }
}